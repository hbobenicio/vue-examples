{
  "name": "my-lib",
  "description": "A Vue.js Library project",
  "version": "0.1.0",
  "author": "Hugo Ben√≠cio <hbobenicio@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.dev.config.js --progress --hide-modules",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --progress --hide-modules",
    "build:all": "npm run build:dev && npm run build:prod"
  },
  "dependencies": {
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
